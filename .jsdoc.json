{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["./src/recordApp", "package.json", "README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules|docs|dist|coverage|tests|middleware|lib|data)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true
    },
    "opts": {
        "destination": "./quality/docs/",
        "encoding": "utf8",
        "private": true,
        "recurse": true
    }
}
